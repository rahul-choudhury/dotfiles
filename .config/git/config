    ai = add --interactive
    ap = add --patch
    ca = commit --amend
    ce = commit --amend --no-edit
    ri = rebase --interactive
    rc = rebase --continue
    ra = rebase --abort
    czz = stash --include-untracked
    czP = stash pop
